{
  "name": "sleepy-spider-front",
  "description": "A creative experiment",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "base-directory": ".",
  "dependencies": {
    "@nidstang/result": "^1.0.3",
    "canvas-tint-image": "^2.1.0",
    "clicks-checker": "^0.1.0",
    "firebase": "^9.13.0",
    "get-canvas-context": "^1.0.2",
    "sleepy-spider-lib": "1.9.0",
    "tweakpane": "^3.1.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.37.0",
    "@rollup/plugin-alias": "^4.0.2",
    "@testing-library/dom": "^9.2.0",
    "@vitest/coverage-v8": "^0.34.2",
    "happy-dom": "^9.7.1",
    "vite": "^3.1.0",
    "vitest": "^0.33.0"
  },
  "scripts": {
    "dev": "vite --mode dev --config ./config/vite.config.js",
    "dev:debug": "vite --mode debug --config ./config/vite.config.js",
    "dev:test:e2e": "vite --mode test --port 8001 --config ./config/vite.config.js",
    "build": "vite build --config ./config/vite.config.js",
    "preview": "vite preview --config ./config/vite.config.js",
    "test": "vitest --config ./config/vite.config.js",
    "coverage": "vitest run --coverage --config ./config/vite.config.js",
    "test:e2e": "npm run dev:test:e2e & ./launch-server.sh & npx playwright test",
    "test:e2e:ui": "npm run test:e2e -- --ui"
  },
  "keywords": [
    "creativecode",
    "canvas-sketch",
    "spider"
  ],
  "author": "ZenekeZene",
  "license": "ISC",
  "engines": {
    "node": "18.12.1"
  }
}
